        "name": "countdown-target",
        "label": "Countdown DateTime",
        "valueType": "date-time",
        "placeholder": "YYYY-MM-DD HH:mm:ss",
        "condition": {
          "or": [
            {
              "==": [
                {
                  "var": "banner-type"
                },
                "countdown"
              ]
            },
            {
              "==": [
                {
                  "var": "banner-type"
                },
                "countdown-big"
              ]
            }
          ]
        }
      },
      {
        "component": "text",
        "name": "subtext",
        "label": "Sub Text",
